<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OpenEMR Suite">


	<groups>
		<run>
			<include name="login"></include>
		</run>
	</groups>

	<parameter name="browser" value="ch"></parameter>
	<test name="Open EMR Test - CH">
		<classes>
			<class name="com.brillio.test.LoginUITest" />
			<class name="com.brillio.test.LoginTest" />
			<class name="com.brillio.test.PatientTest" />
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->